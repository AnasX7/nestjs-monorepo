{
  "name": "my-turborepo",
  "version": "0.0.0",
  "private": true,
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:studio": "turbo -F @repo/db db:studio",
    "db:generate": "turbo -F @repo/db db:generate",
    "db:migrate": "turbo -F @repo/db db:migrate",
    "db:start": "turbo -F @repo/db db:start",
    "db:watch": "turbo -F @repo/db db:watch",
    "db:stop": "turbo -F @repo/db db:stop",
    "db:down": "turbo -F @repo/db db:down"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "prettier": "^3.7.1",
    "turbo": "^2.6.1",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=18"
  }
}
